}
button.trade-btn {float: left;margin-left: 15px; border-radius: 5px; margin-right: 0; padding: 0px 2px 0 10px; height: 30px; display: flex; vertical-align: middle; align-items: center;}
button.trade-btn i { margin-right: 12px; font-size: 18px;}
button.trade-btn .icon {background: #ccc; padding: 0 6px;margin-left: 10px;}

.filter_icons {
    float: right; 
}
 ion-content
{ 
	
