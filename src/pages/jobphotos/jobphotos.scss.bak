	font-size:-1+$value15;
	
}
p
{
	margin-bottom:$value5
	
}

	
	
	
}


}
.trade-btn
{
	&:first-child
	{

	span
   {
	&.hideall 
	{
		margin-top: $value10;
		padding: $value5 $value10;
		right:inherit;
		display:block;
		left:$value0;
		text-transform: capitalize; 
		
	&:after
	{
    left: 2px ;
    right:inherit;
	}	
	}
	
}
	}
}
span
{
	&.hideall 
	{
		    margin-top: $value10;
			padding: $value5 $value10;
		right:$value0;
		left:inherit;
	   display:block;
		
	&:after
	{
    content: ' ';
    position: absolute;
    width: $value0;
    height: $value0;
    /* left: $value0; */
    right:2px;
    top: -4px;
    border: 4px solid;
    border-color: #1976d2 #1976d2 transparent transparent;
    transform: rotate(-45deg);
    margin: 0 auto;

   }
}	
}	